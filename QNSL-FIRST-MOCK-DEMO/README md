response = requests.get(FIGSHARE_URL)
if response.status_code == 200:
  with open("GAIA-OS-QNSL-main_1.zip", "wb") as f:
      f.write(response.content)
  print("Χ”Χ§Χ•Χ‘Χ¥ Χ”Χ•Χ¨Χ“ Χ‘Χ”Χ¦ΧΧ—Χ”")
else:
  print("Χ‘ΧΆΧ™Χ” Χ‘Χ”Χ•Χ¨Χ“Χ Χ”Χ§Χ•Χ‘Χ¥")


